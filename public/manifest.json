{
  "manifest_version": 3,
  "version": "0.1.0",
  "name": "MeTLDR",
  "action": {
    "default_title": "Click to open side panel"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "sidePanel"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
